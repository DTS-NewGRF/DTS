switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_cargo_subtype_text, cargo_subtype) {
    1: return string(STR_REFIT_LIVERY_AD_Tranz_CAF_A_Tung_Chung);  // 뚱충선
    return string(STR_REFIT_LIVERY_AD_Tranz_CAF_A_Airport_Express);
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_engineH, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_engineH;   // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_engineH;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_engineT, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_engineT;   // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_engineT;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_engine, position_in_consist_from_end == 0 && num_vehs_in_consist > 1){
    1: sw_AD_Tranz_CAF_A_engineT;
    sw_AD_Tranz_CAF_A_engineH;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_panto_wagon, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_panto_wagon;   // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_panto_wagon;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon_front, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_wagon; // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_wagon;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon_tail, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_wagon_r;   // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_wagon_r;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_panto_wagon_r, cargo_subtype) {
    1: set_AD_Tranz_CAF_A_Tung_Chung_panto_wagon_r; // 뚱충선
    set_AD_Tranz_CAF_A_Airport_Express_panto_wagon_r;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon_middle, (position_in_consist)) {
    1: sw_AD_Tranz_CAF_A_wagon_tail;
    sw_AD_Tranz_CAF_A_wagon_front;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon_rear, [STORE_TEMP(1, 0x10F), var[0x61, 0, 0x0000FFFF, 0xC6]]){
    AD_Tranz_CAF_A: sw_AD_Tranz_CAF_A_panto_wagon_r;
    sw_AD_Tranz_CAF_A_wagon_middle;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon, [STORE_TEMP(-1, 0x10F), var[0x61, 0, 0x0000FFFF, 0xC6]]) {
    AD_Tranz_CAF_A: sw_AD_Tranz_CAF_A_panto_wagon;
    sw_AD_Tranz_CAF_A_wagon_rear;
}

switch(FEAT_TRAINS, SELF, sw_AD_Tranz_CAF_A_wagon_attach, vehicle_type_id) {
    DTS_Passenger_Car:    return CB_RESULT_ATTACH_ALLOW;
    return string(STR_ERROR_NO_ATTACH);
}
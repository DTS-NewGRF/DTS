    2: return string(STR_REFIT_LIVERY_Line1_K1000_Initial_Aircon_B);
    3: return string(STR_REFIT_LIVERY_Line1_K1000_Old_A);
    4: return string(STR_REFIT_LIVERY_Line1_K1000_Old_B);
    5: return string(STR_REFIT_LIVERY_Line1_S1000);
    6: return string(STR_REFIT_LIVERY_Line1_S1000_Aircon);

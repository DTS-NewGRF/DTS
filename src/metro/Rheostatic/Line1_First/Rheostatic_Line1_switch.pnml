switch(FEAT_TRAINS, SELF, sw_Rheostatic_Line1_6car_cargo_subtype_text, cargo_subtype) {
    1: return string(STR_REFIT_LIVERY_Line1_S1000);
    return string(STR_REFIT_LIVERY_Line1_K1000_Initial);
}

switch(FEAT_TRAINS, SELF, sw_Rheostatic_Line1_10car_cargo_subtype_text, cargo_subtype) {

    return CB_RESULT_NO_SOUND;
    }

switch(FEAT_TRAINS, SELF, sw_Seoulmetro_2nd_VVVF_IGBT_sound, extra_callback_info1) {
    SOUND_EVENT_START: sound("sounds/WOOJINTOSHIBAIGBTstart.wav");
    return CB_RESULT_NO_SOUND;
